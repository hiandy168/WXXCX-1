
.setting{
    flex: 15;
}
.setting view{
    padding-top: 25rpx;
    width: 40rpx;
    height: 40rpx;
    background: url("../../images/setting.gif") no-repeat;
    background-size: 35rpx 40rpx;
    box-sizing: border-box
}

.logged,.not_logged_in{
    width: 100%;
}
.pageContainer .logged image{
    width: 200rpx;
    height: 200rpx;
    border-radius: 50%;
}
.logged text{
    font-size: 35rpx;
    display: block;
    text-align: center;
    color:#2cc17b;
    line-height: 100rpx;
}
.userLogin{
    display: flex;
    flex-direction: column;
    background: #2cc17b;
    width: 100%;
    height: 340rpx;
    padding-top: 90rpx;
}

.userLogin view{
    display: flex;
    width: 140rpx;
    height: 140rpx;
    background: #fff;
    border-radius: 50%;
    justify-content: center;
    align-items: center;
    margin: 0 auto;
    margin-bottom: 40rpx;
}
.userLogin image{
    width: 90rpx;
    height: 90rpx;
    justify-content: center;
    align-items: center;
    margin: 0 auto;
}

.userLogin text{
    font-size: 28rpx;
    height: 60rpx;
    line-height: 60rpx;
    color: #fff;
    justify-content: center;
    align-items: center;
    margin: 0 auto;
}

.loginTo{
    width:100%;
    height: 300rpx;
    box-sizing: border-box;
    padding: 70rpx 50rpx 0;
}

.loginTo button{
    width: 100%;
    font-size: 30rpx;
    height: 80rpx;
    line-height: 80rpx;
    margin-bottom: 30rpx;
}